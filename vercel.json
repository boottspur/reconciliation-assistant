{
  "version": 2,
  "buildCommand": "cd frontend && npm install && npm run build",
  "outputDirectory": "frontend/build",
  "framework": null,
  "routes": [
    {
      "src": "^/$",
      "status": 404
    },
    {
      "src": "^/reconciliation-assistant/?$",
      "dest": "/index.html"
    },
    {
      "src": "^/reconciliation-assistant/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "^/reconciliation-assistant/(.*)",
      "dest": "/$1"
    },
    {
      "src": "^/(?!reconciliation-assistant).*$",
      "status": 404
    }
  ]
}