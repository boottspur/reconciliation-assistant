{
  "version": 2,
  "buildCommand": "cd frontend && npm install && npm run build",
  "outputDirectory": "frontend/build",
  "framework": null,
  "routes": [
    {
      "src": "^/$",
      "status": 404
    },
    {
      "src": "^/reconciliation-assistant/?$",
      "dest": "/hub.html"
    },
    {
      "src": "^/reconciliation-assistant/demo/?$",
      "dest": "/index.html"
    },
    {
      "src": "^/reconciliation-assistant/demo/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "^/reconciliation-assistant/demo/(.*)",
      "dest": "/$1"
    },
    {
      "src": "^/reconciliation-presentation\\.html$",
      "dest": "/reconciliation-presentation.html"
    },
    {
      "src": "^/reconciliation-assistant/prd/?$",
      "dest": "/prd.html"
    },
    {
      "src": "^/(?!reconciliation-assistant|reconciliation-presentation).*$",
      "status": 404
    }
  ]
}